# Environment
APP_ENV=development

# Database
DB_TYPE=postgresql
DB_NAME=ai_orchestrator
DB_HOST=localhost
DB_PORT=5432
DB_USER=postgres
DB_PASSWORD=IknowNothing
DB_SCHEMA=public

# Database Pool Settings
DB_POOL_SIZE=5
DB_MAX_OVERFLOW=10
DB_POOL_TIMEOUT=30
DB_POOL_RECYCLE=1800

# LM Studio Configuration
LMSTUDIO_BASE_URL=http://localhost:1234/v1

# Model Configuration
ATLAS_MODEL=lmstudio-community/Phi-3.1-mini-128k-instruct-GGUF/Phi-3.1-mini-128k-instruct-Q4_K_M.gguf
NOVA_MODEL=lmstudio-community/Phi-3.1-mini-128k-instruct-GGUF/Phi-3.1-mini-128k-instruct-Q4_K_M.gguf:2
SAGE_MODEL=lmstudio-community/Phi-3.1-mini-128k-instruct-GGUF/Phi-3.1-mini-128k-instruct-Q4_K_M.gguf:3
ECHO_MODEL=lmstudio-community/Phi-3.1-mini-128k-instruct-GGUF/Phi-3.1-mini-128k-instruct-Q4_K_M.gguf:4
PIXEL_MODEL=lmstudio-community/Phi-3.1-mini-128k-instruct-GGUF/Phi-3.1-mini-128k-instruct-Q4_K_M.gguf:5
QUANTUM_MODEL=lmstudio-community/Phi-3.1-mini-128k-instruct-GGUF/Phi-3.1-mini-128k-instruct-Q4_K_M.gguf:6

# Service Configuration
ATLAS_PORT=8000
NOVA_PORT=8001
ECHO_PORT=8002
PIXEL_PORT=8003
SAGE_PORT=8004
QUANTUM_PORT=8005

# RabbitMQ Configuration
RABBITMQ_HOST=localhost
RABBITMQ_PORT=5672
RABBITMQ_USER=guest
RABBITMQ_PASSWORD=guest
RABBITMQ_VHOST=/

# Logging Configuration
LOG_LEVEL=INFO
LOG_FORMAT=detailed  # basic, detailed, or json
LOG_TO_FILE=false
LOG_FILE_PATH=logs/ai_orchestrator.log

# Timeout Settings
REQUEST_TIMEOUT=240
REFLECTION_DEPTH=2
MAX_RETRIES=3

# System Metrics
ENABLE_METRICS=true
METRICS_PORT=9090